{
  "root": "libs/api-city",
  "sourceRoot": "libs/api-city/src",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/api-city/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/api-city"],
      "options": {
        "jestConfig": "libs/api-city/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
